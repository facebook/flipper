        "ext": [
          ".flipper"
        ],
        "icon": "document-icons/document.icns",
        "name": "Flipper Data",
        "role": "Viewer"
      }
    ],
    "linux": {
      "category": "Development"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "extendInfo": {
        "NSUserNotificationAlertStyle": "alert"
      }
    },
    "productName": "Flipper",
    "protocols": {
      "name": "flipper",
      "schemes": [
        "flipper"
      ]
    },
    "win": {
      "publisherName": "Facebook, Inc.",
      "sign": null
    }
  },
